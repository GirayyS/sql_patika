soru 3
update employee set name='iron' where id=1 returning *;
update employee set name='impact' where name='AMANDY' returning *;
update employee set email='safdsf@rew.com' where email='AGILDERS2Q@NATURE.COM' returning *;
update employee set birthday='1987-11-21' where email=null returning *;
update employee set email='rrew@dsf.com' where name='JEMIE' returning *;

soru 4
delete from employee where name='BRIGGS' returning *;
